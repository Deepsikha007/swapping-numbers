#include<stdio.h>
int main() 
{
      double first, second, temp;
      printf("Enter first number: ");
      scanf("%d", &first);
      printf("Enter second number: ");
      scanf("%d", &second);
      temp=first;
      first=second;
      second=temp;
      printf("First nmber after swapping=%d",first);
      printf("\n second nmber after swapping=%d",second);
      return 0;
}
