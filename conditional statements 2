#include<stdio.h>
int main()
{
	printf("Pick any food item: \n1. Brownie,Rs 80\n2. Pizza,Rs 150\n3. Beverages,Rs 50\n4. Noodles,Rs 90\n5. Cake,Rs 110");
	int choice=0;
	scanf("%d",&choice);
	
	switch(choice)
	{
		case 1:
			printf("Food:Brownie\nprice:Rs 80.");
			break;
		case 2:
		    printf("Food:Pizza\nprice:Rs 150.");
		    break;
		case 3:
		    printf("Food:Beverages\nprice:Rs 50.");
		    break;
		case 4:
		    printf("Food:Noodles\nprice:Rs 90.");
		    break;
		case 5:
		    printf("Food:Cake\nprice:Rs 110.");	
		    break;
	}
	
}
	
